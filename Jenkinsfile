pipeline {
    agent any
    stages {
        stage "BUILD"{
            steps{
                sleep 5 
                echo "This is BUILD Stage"
            }
        }
        stage "TEST"{
            steps{
                sleep 5 
                echo "This is TEST Stage"
            }
        }
        stage "DEPLOY"{
            steps{
                sleep 5 
                echo "This is DEPLOY Stage"
            }
        }
    }
}
